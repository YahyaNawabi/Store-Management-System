<% layout('layout/boilerplate') %>
<h1>Logistics</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Carrier</th>
      <th>Tracking Number</th>
      <th>Status</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% logistics.forEach(item => { %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.carrier %></td>
        <td><%= item.trackingNumber %></td>
        <td><%= item.status %></td>
        <td>
          <button>Edit</button>
          <button>Delete</button>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
